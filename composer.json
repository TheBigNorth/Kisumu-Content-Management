{
    "name": "wp/wordpress_composer_starter",
    "description": "WordPress Composer Starter repository for quick-starting WordPress projects and the WordPress MultiSite Network",
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "require": {
        "johnpbloch/wordpress": "~4",
        "wpackagist-plugin/wordpress-seo": "7.*",
        "vlucas/phpdotenv": "^2.4",
        "wpackagist-plugin/custom-post-type-ui": "1.5.*",
        "wpackagist-plugin/ilab-media-tools":"2.1.0"
    },
    "extra": {
        "wordpress-install-dir": "public/wordpress",
        "installer-paths": {
            "public/wp-content/plugins/{$name}": ["type:wordpress-plugin"],
            "public/wp-content/themes/{$name}": ["type:wordpress-theme"]
        }
    }
}
